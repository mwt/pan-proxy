{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/",
      "headers" : { "x-robots-tag": "index, follow" },
      "dest": "https://live-digitalhoot.pantheonsite.io"
    },
    {
      "src": "/:match*",
      "headers" : { "x-robots-tag": "index, follow" },
      "dest": "https://live-digitalhoot.pantheonsite.io/:match*"
    },
    {
      "src": "/:match*/",
      "headers" : { "x-robots-tag": "index, follow" },
      "dest": "https://live-digitalhoot.pantheonsite.io/:match*/"
    }
  ]
}
